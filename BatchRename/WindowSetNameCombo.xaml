<Window x:Class="BatchRename.WindowSetNameCombo"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:BatchRename"
        mc:Ignorable="d"
        Title="Đặt tên cho danh sách" Height="160" Width="270">
    <Canvas>
        <TextBox x:Name="txtName" Canvas.Left="116" TextWrapping="Wrap" Canvas.Top="45" Width="120"/>
        <TextBlock Canvas.Left="17" TextWrapping="Wrap" Text="Tên danh sách:" Canvas.Top="45" Width="87" RenderTransformOrigin="0.231,0.455"/>
        <Button Content="Cancel" IsCancel="True" Canvas.Left="50" Canvas.Top="92" Width="75" x:Name="CancelButton" RenderTransformOrigin="0.987,-3.906"/>
        <Button Content="OK" Canvas.Left="145" Canvas.Top="92" Width="75" x:Name="OkButton" Click="OkButton_Click"/>

    </Canvas>
</Window>
